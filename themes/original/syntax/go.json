{
    "tokenColors": [
        {
            "name": "Go keywords",
            "scope": [
                "keyword.package.go",
                "keyword.control.import.go",
                "keyword.type.go",
                "keyword.var.go",
                "keyword.const.go",
                "keyword.interface.go",
                "keyword.struct.go",
                "keyword.function.go",
                "keyword.control.go",
                "keyword.channel.go",
                "keyword.map.go"
            ],
            "settings": {
                "foreground": "{{ syntax-keyword }}",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Go names",
            "scope": [
                "entity.name.type.go",
                "entity.name.function.go",
                "entity.name.type.any.go",
                "entity.name.type.comparable.go"
            ],
            "settings": {
                "foreground": "{{ syntax-name }}",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Go methods names",
            "scope": [
                "entity.name.function.support.go",
                "entity.name.function.support.builtin.go"
            ],
            "settings": {
                "foreground": "{{ syntax-method-name }}",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Go packages and imports names",
            "scope": [
                "entity.name.import.go",
                "entity.name.type.package.go",
                "variable.other.import.go"
            ],
            "settings": {
                "foreground": "{{ syntax-import }}",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Go built-in types",
            "scope": [
                "storage.type.numeric.go",
                "storage.type.string.go",
                "storage.type.boolean.go",
                "storage.type.byte.go",
                "storage.type.error.go",
                "storage.type.rune.go",
                "storage.type.uintptr.go"
            ],
            "settings": {
                "foreground": "{{ syntax-builtin-type }}",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Go comments",
            "scope": [
                "comment.line.double-slash.go",
                "comment.block.go"
            ],
            "settings": {
                "foreground": "{{ syntax-comment }}",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Go strings",
            "scope": [
                "string.quoted.double.go",
                "string.quoted.raw.go",
                "string.quoted.rune.go",
                "constant.character.escape.go",
                "constant.other.placeholder.go"
            ],
            "settings": {
                "foreground": "{{ syntax-string }}"
            }
        },
        {
            "name": "Go constants and numbers",
            "scope": [
                "constant.numeric.decimal.go",
                "constant.numeric.hexadecimal.go",
                "constant.numeric.octal.go",
                "constant.numeric.binary.go",
                "constant.language.boolean.go",
                "constant.language.null.go",
                "constant.language.iota.go",
                "constant.numeric.decimal.point.go"
            ],
            "settings": {
                "foreground": "{{ syntax-number }}"
            }
        },
        {
            "name": "Go variables",
            "scope": [
                "variable.other.constant.go",
                "variable.other.assignment.go",
                "variable.other.property.go",
                "variable.other.go"
            ],
            "settings": {
                "foreground": "{{ syntax-variable }}"
            }
        },
        {
            "name": "Go labels",
            "scope": [
                "variable.other.label.go"
            ],
            "settings": {
                "foreground": "{{ syntax-label }}"
            }
        },
        {
            "name": "Go operators and punctuation",
            "scope": [
                "keyword.operator.address.go",
                "keyword.operator.channel.go",
                "keyword.operator.decrement.go",
                "keyword.operator.increment.go",
                "keyword.operator.comparison.go",
                "keyword.operator.logical.go",
                "keyword.operator.assignment.go",
                "keyword.operator.arithmetic.go",
                "keyword.operator.arithmetic.bitwise.go",
                "keyword.operator.ellipsis.go",
                "punctuation.terminator.go",
                "punctuation.other.period.go",
                "punctuation.other.comma.go",
                "punctuation.other.colon.go"
            ],
            "settings": {
                "foreground": "{{ syntax-operator }}"
            }
        }
    ]
}